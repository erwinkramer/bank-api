  "components": {
    "schemas": {
      "GenericString": {
        "maxLength": 70707070,
        "pattern": ".*",
        "type": "string"
      },
      "GenericInt": {
        "maximum": 70707070,
        "minimum": 0,
        "type": "integer",
        "format": "int32"
      },
      "Problem": {
        "type": "object",
        "properties": {
          "type": {
            "$ref": "#/components/schemas/GenericString"
          },
          "title": {
            "$ref": "#/components/schemas/GenericString"
