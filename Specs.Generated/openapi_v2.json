          },
          "requestId": {
            "$ref": "#/components/schemas/GenericString"
          }
        }
      },
      "BankModel": {
        "type": "object",
        "properties": {
          "id": {
            "maxLength": 36,
            "type": "string",
            "description": "Id of the bank.",
            "format": "uuid"
          },
          "name": {
            "maxLength": 70707070,
            "pattern": ".*",
            "type": "string",
            "description": "Name of the bank.",
            "default": "Guanchen",
            "nullable": true
          },
          "isCompliant": {
            "type": "boolean",
            "description": "Compliancy status of the bank.",
            "default": true
          },
          "bankTier": {
            "$ref": "#/components/schemas/BankTier"
          }
        },
        "example": {
          "id": "24d1f3c4-a1d3-477f-9ca6-b611997e8343",
          "name": "Guanchen",
          "isCompliant": true,
          "bankTier": "A"
        }
      },
      "BankTier": {
        "enum": [
          "A",
          "B",
          "C"
        ],
        "description": "Tier of the bank.",
        "default": "A"
      },
      "PagingOfBankModel": {
        "type": "object",
        "properties": {
          "count": {
            "maximum": 70707070,
            "minimum": 0,
            "type": "integer",
            "format": "int32"
          },
          "data": {
            "maxItems": 70707070,
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/BankModel"
            }
          }
        },
        "example": {
          "count": 2,
          "data": [
            {
              "id": "24d1f3c4-a1d3-477f-9ca6-b611997e8343",
              "name": "Foo",
              "isCompliant": true,
              "bankTier": "A"
            },
            {
              "id": "956b7056-6e98-4a92-af07-91634ce4d421",
              "name": "Bar",
              "isCompliant": false,
              "bankTier": "B"
            }
          ]
        }
      },
      "Teller": {
        "type": "object",
        "properties": {
          "gitHubProfile": {
            "maxLength": 70707070,
            "type": "string",
            "description": "GitHub profile of the teller.",
            "format": "uri",
            "nullable": true
          }
        },
        "example": {
          "gitHubProfile": "https://github.com/rbhanda"
        }
      },
      "TellerReport": {
        "type": "object",
        "properties": {
          "name": {
            "maxLength": 70707070,
            "pattern": ".*",
            "type": "string",
            "description": "Name of the report.",
            "nullable": true
          }
        }
      },
      "TellerReportList": {
        "type": "object",
        "properties": {
          "data": {
            "maxItems": 70707070,
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/TellerReport"
            }
          },
          "count": {
            "maximum": 70707070,
            "minimum": 0,
            "type": "integer",
            "format": "int32"
          }
        },
        "example": {
          "count": 2,
          "data": [
            {
              "name": "reportOfTheWeek.txt"
            },
            {
              "name": "dailyReview.txt"
            }
          ]
        }
      }
    },
